t sum(a.value) from a where (a.su_subscription_month != a.wo_operation_month) and (a.su_termination_month >= a.wo_operation_month) ),0) as regular,\\n    coalesce((select sum(a.value) from a where (a.su_subscription_month = a.wo_operation_month) and (a.su_termination_month != a.wo_operation_month) and (a.ua_creation_month = a.su_subscription_month)),0) as new,\\n    coalesce((select sum(a.value) from a where (a.su_subscription_month = a.wo_operation_month) and (a.su_termination_month != a.wo_operation_month) and (a.ua_creation_month != a.su_subscription_month)),0) as upsell,\\n    coalesce((select sum(a.value) from a where (a.su_termination_month = a.wo_operation_month)),0) as cancelled\\n) as pastmonth,\\n(\\nwith a as\\n(\\n    SELECT round(sum(wo.amount_without_tax),2) as value ,\\n\\tdate_trunc('month', coalesce(ua.subscription_date, date '2000-01-01')) as ua_creation_month,\\n\\t\\t   date_trunc('month', coalesce(s.subscription_date, date '2000-01-01')) as su_subscription_month,\\n\\t\\t   date_trunc('month', coalesce(s.termination_date, date '3000-01-01')) as su_termination_month,\\n\\t\\t   date_trunc('month', coalesce(wo.operation_date, date '2000-01-01')) as wo_operation_month\\n    FROM\\n        billing_wallet_operation wo\\n        inner join billing_charge_instance ci on ci.id = wo.charge_instance_id\\n        inner join cat_recurring_charge_templ rc on rc.id = ci.charge_template_id\\n        inner join billing_subscription s on s.id = ci.subscription_id\\n        inner join billing_user_account ua on ua.id = s.user_account_id\\n\\n    WHERE\\n    \\tdate_trunc('month', wo.operation_date)=date_trunc('month', to_date('#{date}','YYYY-MM-DD') )\\n\\tAND wo.end_date is not null\\n\\tand wo.operation_type='W'\\t\\n    GROUP BY\\n\\t\\tua_creation_month,\\n\\t   su_subscription_month,\\n\\t   su_termination_month,\\n\\t   wo_operation_month\\n    )\\nselect\\n    coalesce((select sum(a.value) from a),0) as total,\\n    coalesce((select sum(a.value) from a where (a.su_subscription_month != a.wo_operation_month) and (a.su_termination_month >= a.wo_operation_month)),0) as regular,\\n    coalesce((select sum(a.value) from a where (a.su_subscription_month = a.wo_operation_month) and (a.su_termination_month != a.wo_operation_month) and (a.ua_creation_month = a.su_subscription_month)),0) as new,\\n    coalesce((select sum(a.value) from a where (a.su_subscription_month = a.wo_operation_month) and (a.su_termination_month != a.wo_operation_month) and (a.ua_creation_month != a.su_subscription_month)),0) as upsell,\\n    coalesce((select sum(a.value) from a where (a.su_termination_month = a.wo_operation_month)),0) as cancelled\\n) as currmonth\\n\",\r\n        \"measurementPeriod\": \"MONTHLY\",\r\n        \"lastMeasureDate\":\"2017-01-01\"\r\n    }",
