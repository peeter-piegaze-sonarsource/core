				"id": "e91363f1-9973-4ce8-9791-fb5436affa6c",
				"type": "text/javascript",
				"exec": [
					"var parts = pm.info.requestName.split(\"|\");",
					"var action = parts[0];",
					"var entity = parts[1];",
					"var suffix = parts[2];",
					"var status = parts[3];",
					"pm.variables.set(\"action\", action);",
					"",
					"pm.variables.set(\"entity\", entity);",
					"pm.variables.set(\"entityRead\", entity.charAt(0).toLowerCase() + entity.slice(1));",
					"pm.variables.set(\"codeIdentifier\", \"code\");",
					"pm.variables.set(\"haveDescription\", true);",
					"pm.variables.set(\"haveCode\", true);",
					"",
					"pm.variables.set(\"suffix\", suffix);",
					"var entityCode = pm.collectionVariables.get(\"testCode\")+\"_\"+suffix;",
					"pm.variables.set(\"entityCode\", entityCode);",
					"pm.variables.set(\"description\", pm.collectionVariables.get(\"testDescription\"));",
					"",
					"pm.variables.set(\"messageEql\", true);",
					"pm.variables.set(\"status\", status);",
					"switch(status){",
					"    case \"SUCCESS\": {",
					"        pm.variables.set(\"code\", 200);",
					"        (action.includes(\"Read\") ? pm.variables.set(\"message\", ) : pm.variables.set(\"message\", \"\"));",
					"        pm.variables.set(\"status\", status);",
					"        pm.variables.set(\"errorCode\", );",
					"        break;        ",
					"    };",
					"    case \"ENTITY_ALREADY_EXISTS_EXCEPTION\": {",
					"        pm.variables.set(\"code\", 403);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        pm.variables.set(\"message\",entity+\" with code=\"+entityCode+\" already exists.\");",
					"        break;",
					"    };",
					"    case \"ENTITY_DOES_NOT_EXISTS_EXCEPTION\": {",
					"        pm.variables.set(\"code\", 404);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        (action==\"Delete\"",
					"        ? pm.variables.set(\"message\", entity+\" with code=\"+entityCode+\" does not exists.\")",
					"        : pm.variables.set(\"message\", entity+\" with code=NOT_EXIST does not exists.\"));",
					"        break;",
					"    };",
					"    case \"MISSING_PARAMETER\":{",
					"        pm.variables.set(\"code\", 400);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        break;",
					"    };",
					"    case \"INVALID_PARAMETER\": {",
					"        pm.variables.set(\"code\", 400);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        pm.variables.set(\"messageEql\", false);",
					"        break;",
					"    };",
					"    case \"INTERNAL_SERVER_ERROR\":{",
					"        pm.variables.set(\"code\", 500);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", );",
					"        pm.variables.set(\"errorCode\", );",
					"        break;",
					"    }",
					"    case \"GENERIC_API_EXCEPTION\":{",
					"        pm.variables.set(\"code\", 500);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", );",
					"        pm.variables.set(\"errorCode\", status);",
					"        break;",
					"    }",
					"    case \"BUSINESS_API_EXCEPTION\":{",
					"        pm.variables.set(\"code\", 500);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        break;",
					"    }",
					"    case \"DELETE_REFERENCED_ENTITY_EXCEPTION\":{",
					"        pm.variables.set(\"code\", 403);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status);",
					"        break;",
					"    }",
					"    default:{",
					"        pm.variables.set(\"code\", 400);",
					"        pm.variables.set(\"status\", \"FAIL\");",
					"        pm.variables.set(\"errorCode\", status); ",
					"    }",
					"}",
					"",
					""
				]
			}
